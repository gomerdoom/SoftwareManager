@page
@model NewerSoftwareModel
@{
    ViewData["Title"] = "Newer Software";
}

<div class="text-center">
    <h1 class="display-4">Software</h1>
    
    <table class="table">
        <thead>
            <tr>
                <th>
                    Name
                </th>
                <th>
                    Version
                </th>
            </tr>
        </thead>
        <tbody>
            @foreach (var software in Model.NewerSoftware) {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => software.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => software.Version)
                    </td>
                </tr>
            }
        </tbody>
    </table>
</div>
